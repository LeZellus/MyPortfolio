        <meta property="og:image" content="{{ asset('uploads/metatag.png') }}" />

        <!-- Twitter -->
        <meta property="twitter:card" content="summary_large_image" />
        <meta property="twitter:url" content="https://matheozeller.fr/" />
        <meta property="twitter:title" content="Mathéo Zeller | Développeur Pixel" />
        <meta property="twitter:description" content="Découvrez Mathéo Zeller, développeur innovant : portfolio de projets web, mobiles et embarqués, mêlant technologie avancée et créativité unique." />
        <meta property="twitter:image" content="{{ asset('uploads/metatag.png') }}" />

        <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('uploads/portfolio_icon.png') }}">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('uploads/portfolio_icon.png') }}">

        <title>Mathéo Zeller | Développeur Pixel !</title>
