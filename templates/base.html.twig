        <!-- Primary Meta Tags -->
        <meta name="title" content="Mathéo Zeller | Développeur Pixel" />
        <meta name="description" content="Découvrez Mathéo Zeller, développeur innovant : portfolio de projets web, mobiles et embarqués, mêlant technologie avancée et créativité unique." />

        <!-- Open Graph / Facebook -->
        <meta property="og:type" content="website" />
        <meta property="og:url" content="https://matheozeller.fr/" />
        <meta property="og:title" content="Mathéo Zeller | Développeur Pixel" />
